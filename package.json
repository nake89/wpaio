{
    "name": "svelte-app",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "start": "sirv public --no-clear",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "noBuildDeploy": "aws s3 sync public/ s3://wpaio --profile wpaio-s3",
        "deploy": "npm run build && npm run noBuildDeploy && npm run invalidate",
        "prettier": "prettier",
        "format": "prettier --write src/*",
        "invalidate": "aws cloudfront create-invalidation --distribution-id E3OKZ9PD2J06L0 --paths \"/*\" --profile cloudfront-invalidation",
        "amplify-modelgen": "node amplify/scripts/amplify-modelgen.js",
        "amplify-push": "node amplify/scripts/amplify-push.js"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "@rollup/plugin-typescript": "^8.0.0",
        "@tsconfig/svelte": "^2.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.0.0",
        "svelte-check": "^2.0.0",
        "svelte-preprocess": "^4.0.0",
        "tslib": "^2.0.0",
        "typescript": "^4.0.0",
        "ini": "^1.3.5",
        "inquirer": "^6.5.1"
    },
    "dependencies": {
        "@aws-amplify/auth": "^4.3.15",
        "@aws-amplify/core": "^4.3.7",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "aws-amplify": "^4.3.7",
        "node-sass": "^6.0.1",
        "sirv-cli": "^1.0.0",
        "spaper": "^0.1.2",
        "svelte-fa": "^2.3.3"
    }
}